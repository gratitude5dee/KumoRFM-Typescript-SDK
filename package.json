{
  "name": "@kumo-ai/rfm-sdk",
  "version": "0.0.0",
  "description": "KumoRFM TypeScript SDK",
  "license": "Apache-2.0",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    }
  },
  "engines": {
    "node": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gratitude5dee/KumoRFM-Typescript-SDK.git"
  },
  "bugs": {
    "url": "https://github.com/gratitude5dee/KumoRFM-Typescript-SDK/issues"
  },
  "homepage": "https://github.com/gratitude5dee/KumoRFM-Typescript-SDK#readme",
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --clean",
    "test": "vitest run --passWithNoTests",
    "lint": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "docs": "typedoc --out docs src/index.ts"
  },
  "dependencies": {},
  "devDependencies": {
    "typescript": "^5.6.2",
    "tsup": "^8.2.4",
    "vitest": "^2.1.1",
    "eslint": "^9.9.0",
    "typescript-eslint": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.3.3",
    "typedoc": "^0.26.10"
  },
  "packageManager": "npm@10"
}
